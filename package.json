{
  "name": "myapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "concurrently --kill-others-on-fail \"yarn start --reset-cache\" \"react-native run-android && react-native log-android\" \"yarn stay-awake-android\"",
    "ios": "concurrently --kill-others-on-fail \"yarn start --reset-cache\" \"react-native run-ios\"",
    "start": "react-native start",
    "web": "webpack serve --mode development",
    "build": "craco build",
    "test": "craco test --env=jsdom",
    "lint": "tsc && eslint '**/*.[jt]s?(x)' --cache --fix",
    "clean": "rm -rf node_modules && yarn && concurrently --kill-others-on-fail \"yarn clean-android\" \"yarn clean-ios\"",
    "clean-android": "cd android && rm -rf ./android/build && rm -rf ./android/.gradle && rm -rf ./android/app/build && ./gradlew clean && cd ..",
    "clean-ios": "rm -rf ~/Library/Developer/Xcode/DerivedData && cd ios && rm -rf build Pods && pod install && cd ..",
    "generate": "plop --plopfile ./.plop/plop.config.js",
    "storybook": "start-storybook -p 7007"
  },
  "dependencies": {
    "@craco/craco": "6.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.64.1",
    "react-native-paper": "^4.8.1",
    "react-native-vector-icons": "^8.1.0",
    "react-native-web": "^0.16.2",
    "react-scripts": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-flow": "^7.13.13",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/react": "^6.2.9",
    "@storybook/react-native": "^5.3.25",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.5",
    "@types/react-native": "^0.64.4",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/react-test-renderer": "^17.0.1",
    "@types/storybook__addon-info": "^5.2.3",
    "@types/storybook__addon-knobs": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-react-native-web": "^0.16.2",
    "copy-webpack-plugin": "^8.1.1",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-native-typescript": "^2.2.6",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "metro-react-native-babel-preset": "^0.66.0",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^2.0.0",
    "react-test-renderer": "^17.0.2",
    "storybook-dark-mode": "^1.0.8",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "resolutions": {
    "@types/react": "17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
